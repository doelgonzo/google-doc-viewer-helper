{
  "name": "google-doc-viewer-helper",
  "version": "1.0.2",
  "author": "Doel L Gonzalez II <doel.gonzo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/doelgonzo/google-doc-viewer-helper",
  "description": "Helper utilities for Google Doc Viewer",
  "main": "src/gdoc-viewer-helper.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha --require @babel/register \"src/**/**.test.js\"",
    "build:clean": "rimraf ./gdoc-viewer-helper.min.js",
    "prebuild": "npm run build:clean",
    "build": "browserify ./src/gdoc-viewer-helper.js | uglifyjs -mc > ./gdoc-viewer-helper.min.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@sinonjs/referee-sinon": "^4.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsverify": "^0.8.3",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "sinon": "^6.3.3",
    "sinon-chai": "^3.2.0",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
